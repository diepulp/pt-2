# E2E Test Environment Configuration
# Copy this to .env.test and fill in your local Supabase credentials

# Next.js app URL for E2E tests
BASE_URL=http://localhost:3000

# Supabase connection (use local Supabase instance for tests)
NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-local-anon-key

# Service role key for test data setup/teardown (bypasses RLS)
# WARNING: Never commit this key or use production credentials
SUPABASE_SERVICE_ROLE_KEY=your-local-service-role-key
