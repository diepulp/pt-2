{
  "private": true,
  "scripts": {
    "dev:kill": "uv run scripts/dev-cleanup.sh",
    "dev": "npm run dev:kill && next dev --webpack",
    "dev:webpack": "next dev --webpack",
    "build": "next build",
    "build:webpack": "next build --webpack",
    "start": "next start",
