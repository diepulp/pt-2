    "openapi:client": "openapi-generator-cli generate -g typescript-fetch",
    "openapi:client:axios": "openapi-generator-cli generate -g typescript-axios",
    "openapi:validate": "openapi-generator-cli validate -i docs/api-route-catalogue/api-surface.openapi.yaml",
    "openapi:sync": "cp docs/api-route-catalogue/api-surface.openapi.yaml public/api-spec.yaml",
    "openapi:all": "npm run openapi:validate && npm run openapi:types && npm run openapi:client && npm run openapi:sync"
